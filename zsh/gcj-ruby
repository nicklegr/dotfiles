# GCJ Ruby
function go()
{
    if ! [ -e $1.rb ]; then
        cp tmpl.rb $1.rb
    fi

    sublime $1.rb
    touch $1-sample.in
    sublime $1-sample.in
}

function run()
{
    ruby $1.rb < $2
}

function debug()
{
    ruby --debug $1.rb < $2
}

function sample()
{
    ruby --debug $1.rb < $1-sample.in
}

